                             <%
                             // Define the maximum length for the truncated description
                             const maxLength = 100;

                             // Check if the description is longer than the maximum length
                             if (animeData.description.length > maxLength) {
                                 // If it is, truncate the description and add an ellipsis
                                 var truncatedDescription = animeData.description.substring(0, maxLength);
                                 var showMore = true;
                             } else {
                                 // If it's not, use the original description and hide the "Show More" button
                                 var truncatedDescription = animeData.description;
                                 var showMore = false;
                             }
                             %>
                                 <div id="truncatedDescription" style="display:block"><%- truncatedDescription %><% if (showMore) { %>
                                      <button id="showMoreBtn">...</button>
                                      <% } %></div>
                                     <!-- Show More button (conditionally displayed) -->

